00065 {\cf17 function} export_enquiries_to_csv() \{\par
00066     {\cf19 try} \{\par
00067         {\cf20 // Verify user permissions}\par
00068         {\cf19 if} (!current_user_can({\cf22 'manage_options'})) \{\par
00069             {\cf19 throw} {\cf17 new} Exception({\cf22 'Insufficient permissions to export data.'});\par
00070         \}\par
00071 \par
00072         global $wpdb;\par
00073         \par
00074         {\cf20 // Get and sanitize parameters}\par
00075         $status = isset($_GET[{\cf22 'status'}]) ? sanitize_text_field($_GET[{\cf22 'status'}]) : {\cf22 'all'};\par
00076         $start_date = isset($_GET[{\cf22 'start_date'}]) ? sanitize_text_field($_GET[{\cf22 'start_date'}]) : {\cf22 ''};\par
00077         $end_date = isset($_GET[{\cf22 'end_date'}]) ? sanitize_text_field($_GET[{\cf22 'end_date'}]) : {\cf22 ''};\par
00078 \par
00079         {\cf20 // Define table names}\par
00080         $enquiries_table = $wpdb->prefix . {\cf22 'ef_enquiries'};\par
00081         $cart_items_table = $wpdb->prefix . {\cf22 'ef_cart_items'};\par
00082 \par
00083         {\cf20 // Build the query}\par
00084         $query = {\cf22 "SELECT e.id AS enquiry_id, e.subject, e.content, e.name, e.company, }\par
00085 {\cf22                         e.email, e.phone, e.created_at, e.status, }\par
00086 {\cf22                         ci.product_name, ci.sku}\par
00087 {\cf22                  FROM $enquiries_table AS e}\par
00088 {\cf22                  LEFT JOIN $cart_items_table AS ci ON e.id = ci.enquiry_id}\par
00089 {\cf22                  WHERE 1=1"};\par
00090         \par
00091         $query_params = array();\par
00092 \par
00093         {\cf20 // Add status filter}\par
00094         {\cf19 if} ($status !== {\cf22 'all'}) \{\par
00095             $query .= {\cf22 " AND e.status = %s"};\par
00096             $query_params[] = $status;\par
00097         \}\par
00098 \par
00099         {\cf20 // Add date range filters}\par
00100         {\cf19 if} (!empty($start_date)) \{\par
00101             $query .= {\cf22 " AND e.created_at >= %s"};\par
00102             $query_params[] = $start_date . {\cf22 ' 00:00:00'};\par
00103         \}\par
00104         {\cf19 if} (!empty($end_date)) \{\par
00105             $query .= {\cf22 " AND e.created_at <= %s"};\par
00106             $query_params[] = $end_date . {\cf22 ' 23:59:59'};\par
00107         \}\par
00108 \par
00109         $query .= {\cf22 " ORDER BY e.id, ci.id"};\par
00110 \par
00111         {\cf20 // Prepare and execute the query}\par
00112         $final_query = $query_params ? $wpdb->prepare($query, $query_params) : $query;\par
00113         $results = $wpdb->get_results($final_query, ARRAY_A);\par
00114 \par
00115         {\cf19 if} (!$results) \{\par
00116             {\cf19 throw} {\cf17 new} Exception({\cf22 'No data available for the selected criteria.'});\par
00117         \}\par
00118 \par
00119         {\cf20 // Set headers for CSV export}\par
00120         header({\cf22 'Content-Type: text/csv; charset=utf-8'});\par
00121         header({\cf22 'Content-Disposition: attachment;filename=enquiries_export_'} . date({\cf22 'Y-m-d'}) . {\cf22 '.csv'});\par
00122         header({\cf22 'Pragma: no-cache'});\par
00123         header({\cf22 'Expires: 0'});\par
00124         \par
00125         {\cf20 // Open output stream}\par
00126         $output = fopen({\cf22 'php://output'}, {\cf23 'w'});\par
00127         {\cf19 if} ($output === {\cf17 false}) \{\par
00128             {\cf19 throw} {\cf17 new} Exception({\cf22 'Failed to open output stream'});\par
00129         \}\par
00130         \par
00131         {\cf20 // Write CSV headers}\par
00132         fputcsv($output, array({\cf22 'Enquiry ID'}, {\cf22 'Subject'}, {\cf22 'Content'}, {\cf22 'Name'}, {\cf22 'Company'}, \par
00133                               {\cf22 'Email'}, {\cf22 'Phone'}, {\cf22 'Order Time'}, {\cf22 'Status'}, \par
00134                               {\cf22 'Product Name'}, {\cf22 'SKU'}));\par
00135 \par
00136         {\cf20 // Write data rows}\par
00137         {\cf19 foreach} ($results as $row) \{\par
00138             fputcsv($output, $row);\par
00139         \}\par
00140 \par
00141         fclose($output);\par
00142         \par
00143         {\cf20 // Log successful export}\par
00144         EF_Logger::log({\cf22 'CSV export completed successfully'}, EF_Logger::INFO, [\par
00145             {\cf22 'count'} => count($results),\par
00146             {\cf22 'status_filter'} => $status,\par
00147             {\cf22 'start_date'} => $start_date,\par
00148             {\cf22 'end_date'} => $end_date,\par
00149             {\cf22 'user'} => wp_get_current_user()->user_login\par
00150         ]);\par
00151 \par
